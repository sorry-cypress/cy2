{
  "name": "cy2",
  "version": "1.3.0",
  "main": "index.js",
  "author": "Andrew Goldis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sorry-cypress/cy2.git"
  },
  "scripts": {
    "test": "jest",
    "release": "release-it"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "js-yaml": "^4.0.0",
    "npm-which": "^3.0.1"
  },
  "files": [
    "bin/*",
    "lib/*",
    "index.js"
  ],
  "bin": {
    "cy2": "bin/cy2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "cypress",
    "wrapper",
    "sorry-cypress",
    "currents"
  ],
  "devDependencies": {
    "@release-it/conventional-changelog": "^3.3.0",
    "jest": "^26.6.3",
    "nanoid": "^3.1.25",
    "release-it": "^14.11.5",
    "shelljs": "^0.8.4"
  },
  "release-it": {
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "jest": {
    "testMatch": [
      "./__tests__/*.spec.js",
      "./**/__tests__/*.spec.js"
    ]
  }
}
